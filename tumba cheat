    local function StartFollow(player)
        if player == LocalPlayer then return end
        Mega.States.Player.FollowTarget = player
        showNotification(GetText("notify_follow_start", player.Name))
    end

    local function StopFollow()
        Mega.States.Player.FollowTarget = nil
        Workspace.CurrentCamera.CameraType = Enum.CameraType.Custom
        showNotification(GetText("notify_follow_stop"))
    end

    local function UpdatePlayerList()
        local localHRP = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")

        for player, item in pairs(Mega.Objects.PlayerListItems) do
            if not player or not player.Parent then
                item:Destroy()
                Mega.Objects.PlayerListItems[player] = nil
            end
        end

        for _, player in ipairs(Players:GetPlayers()) do
            if player ~= LocalPlayer then
                local item = Mega.Objects.PlayerListItems[player]

                if not item then
                    item = PlayerItemTemplate:Clone()
                    item.Name = player.Name .. "Item"
                    item.Visible = true
                    item.Parent = PlayerListContainer
                    Mega.Objects.PlayerListItems[player] = item

                    local p = player 
                    item.MouseButton1Click:Connect(function()
                        StartFollow(p)
                    end)
                end

                local char = player.Character
                local humanoid = char and char:FindFirstChildOfClass("Humanoid")
                local hrp = char and char:FindFirstChild("HumanoidRootPart")

                local nameLabel = item:FindFirstChild("Name")
                local teamLabel = item:FindFirstChild("Team")
                local hpLabel = item:FindFirstChild("HP")
                local distLabel = item:FindFirstChild("Distance")

                if Mega.States.Player.FollowTarget == player then
                    item.BackgroundColor3 = Mega.Settings.Menu.AccentColor
                else
                    item.BackgroundColor3 = Color3.fromRGB(30, 35, 50)
                end

                if nameLabel then
                    nameLabel.Text = player.Name
                end

                if teamLabel then
                    teamLabel.Text = (player.Team and player.Team.Name) or GetText("playerlist_team_none") -- Ğ˜Ğ—ĞœĞ•ĞĞ•ĞĞ
                    if player.Team and player.Team.TeamColor then
                        teamLabel.TextColor3 = player.Team.TeamColor.Color
                    else
                        teamLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
                    end
                end

                if hpLabel and humanoid then
                    hpLabel.Text = GetText("playerlist_hp_format", math.floor(humanoid.Health)) -- Ğ˜Ğ—ĞœĞ•ĞĞ•ĞĞ
                    hpLabel.TextColor3 = humanoid.Health > 0 and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 0, 0)
                elseif hpLabel then
                    hpLabel.Text = GetText("playerlist_hp_dead") -- Ğ˜Ğ—ĞœĞ•ĞĞ•ĞĞ
                    hpLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
                end

                if distLabel and localHRP and hrp then
                    local distance = (localHRP.Position - hrp.Position).Magnitude
                    distLabel.Text = GetText("playerlist_dist_format", math.floor(distance)) -- Ğ˜Ğ—ĞœĞ•ĞĞ•ĞĞ
                elseif distLabel then
                    distLabel.Text = GetText("playerlist_dist_none") -- Ğ˜Ğ—ĞœĞ•ĞĞ•ĞĞ
                end
            end
        end

        PlayerListContainer.CanvasSize = UDim2.new(0, 0, 0, ListLayout.AbsoluteContentSize.Y + 20)
    end
    -- *****************************************************************


    --=== ĞœĞ•Ğ“Ğ-Ğ¡Ğ˜Ğ¡Ğ¢Ğ•ĞœĞ ESP ===--
    function CreateESP(player)
        if player == LocalPlayer then return end

        local esp = {
            box = Drawing.new("Square"),
            name = Drawing.new("Text"),
            distance = Drawing.new("Text"),
            health = Drawing.new("Text"),
            tracer = Drawing.new("Line")
        }

        esp.box.Visible = false
        esp.box.Thickness = 2
        esp.box.Filled = false
        esp.box.ZIndex = 1

        esp.name.Visible = false
        esp.name.Size = 14
        esp.name.Center = true
        esp.name.Outline = true
        esp.name.ZIndex = 1

        esp.distance.Visible = false
        esp.distance.Size = 12
        esp.distance.Center = true
        esp.distance.Outline = true
        esp.distance.ZIndex = 1

        esp.health.Visible = false
        esp.health.Size = 12
        esp.health.Center = true
        esp.health.Outline = true
        esp.health.ZIndex = 1

        esp.tracer.Visible = false
        esp.tracer.Thickness = 1
        esp.tracer.ZIndex = 1

        Mega.Objects.ESP[player] = esp
    end
    function RemoveESP(player)
        if Mega.Objects.ESP[player] then
            for _, drawing in pairs(Mega.Objects.ESP[player]) do
                drawing:Remove()
            end
            Mega.Objects.ESP[player] = nil
        end
    end
    function UpdateESPColors()
        for player, esp in pairs(Mega.Objects.ESP) do
            if player and player.Character and player.Team then
                local isTeam = Mega.States.ESP.ShowTeam and player.Team == LocalPlayer.Team
                local isEnemy = player.Team ~= LocalPlayer.Team

                if isTeam then
                    esp.box.Color = Mega.States.ESP.TeamColor
                    esp.name.Color = Mega.States.ESP.TeamColor
                    esp.distance.Color = Mega.States.ESP.TeamColor
                    esp.health.Color = Mega.States.ESP.TeamColor
                    esp.tracer.Color = Mega.States.ESP.TeamColor
                elseif isEnemy then
                    esp.box.Color = Mega.States.ESP.EnemyColor
                    esp.name.Color = Mega.States.ESP.EnemyColor
                    esp.distance.Color = Mega.States.ESP.EnemyColor
                    esp.health.Color = Mega.States.ESP.EnemyColor
                    esp.tracer.Color = Mega.States.ESP.EnemyColor
                else
                    esp.box.Color = Mega.States.ESP.NeutralColor
                    esp.name.Color = Mega.States.ESP.NeutralColor
                    esp.distance.Color = Mega.States.ESP.NeutralColor
                    esp.health.Color = Mega.States.ESP.NeutralColor
                    esp.tracer.Color = Mega.States.ESP.NeutralColor
                end
            end
        end
    end
    function UpdateESP()
        local camera = Workspace.CurrentCamera
        local screenCenter = Vector2.new(camera.ViewportSize.X / 2, camera.ViewportSize.Y)

        for player, esp in pairs(Mega.Objects.ESP) do
            if player and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                local rootPart = player.Character.HumanoidRootPart
                local head = player.Character:FindFirstChild("Head")
                local humanoid = player.Character:FindFirstChild("Humanoid")

                if head and humanoid then
                    local screenPos, onScreen = camera:WorldToViewportPoint(rootPart.Position)
                    local distance = (LocalPlayer.Character.HumanoidRootPart.Position - rootPart.Position).Magnitude

                    if onScreen and distance <= Mega.States.ESP.MaxDistance then
                        local scale = 1000 / distance
                        local width = scale * 2
                        local height = scale * 3

                        if Mega.States.ESP.Boxes then
                            esp.box.Visible = Mega.States.ESP.Enabled
                            esp.box.Position = Vector2.new(screenPos.X - width / 2, screenPos.Y - height / 2)
                            esp.box.Size = Vector2.new(width, height)
                        else
                            esp.box.Visible = false
                        end

                        if Mega.States.ESP.Names then
                            esp.name.Visible = Mega.States.ESP.Enabled
                            esp.name.Position = Vector2.new(screenPos.X, screenPos.Y - height / 2 - 20)
                            esp.name.Text = player.Name
                        else
                            esp.name.Visible = false
                        end

                        if Mega.States.ESP.Distance then
                            esp.distance.Visible = Mega.States.ESP.Enabled
                            esp.distance.Position = Vector2.new(screenPos.X, screenPos.Y + height / 2 + 10)
                            esp.distance.Text = GetText("esp_studs", math.floor(distance)) -- Ğ˜Ğ—ĞœĞ•ĞĞ•ĞĞ
                        else
                            esp.distance.Visible = false
                        end

                        if Mega.States.ESP.Health then
                            esp.health.Visible = Mega.States.ESP.Enabled
                            esp.health.Position = Vector2.new(screenPos.X, screenPos.Y + height / 2 + 30)
                            esp.health.Text = GetText("esp_hp", math.floor(humanoid.Health)) -- Ğ˜Ğ—ĞœĞ•ĞĞ•ĞĞ
                        else
                            esp.health.Visible = false
                        end

                        if Mega.States.ESP.Tracers then
                            esp.tracer.Visible = Mega.States.ESP.Enabled
                            esp.tracer.From = Vector2.new(screenPos.X, screenPos.Y + height / 2)
                            esp.tracer.To = screenCenter
                        else
                            esp.tracer.Visible = false
                        end

                    else
                        esp.box.Visible = false
                        esp.name.Visible = false
                        esp.distance.Visible = false
                        esp.health.Visible = false
                        esp.tracer.Visible = false
                    end
                end
            else
                esp.box.Visible = false
                esp.name.Visible = false
                esp.distance.Visible = false
                esp.health.Visible = false
                esp.tracer.Visible = false
            end
        end
    end
    -- Kit ESP no update needed, since static creation
    --=== ĞœĞ•Ğ“Ğ-Ğ¡Ğ˜Ğ¡Ğ¢Ğ•ĞœĞ AIM ASSIST ===--
    local AimFOVCircle = nil
    local AimLoopConnection = nil
    local CurrentAimTarget = nil
    function CreateAimFOV()
        if AimFOVCircle then
            AimFOVCircle:Remove()
        end

        AimFOVCircle = Drawing.new("Circle")
        AimFOVCircle.Visible = Mega.States.AimAssist.Enabled and Mega.States.AimAssist.ShowFOV
        AimFOVCircle.Color = Mega.States.AimAssist.FOVColor
        AimFOVCircle.Thickness = 2
        AimFOVCircle.NumSides = 100
        AimFOVCircle.Radius = Mega.States.AimAssist.FOV
        AimFOVCircle.Filled = false
    end
    function GetClosestPlayer()
        local closestPlayer = nil
        local closestDistance = Mega.States.AimAssist.FOV

        for _, player in pairs(Players:GetPlayers()) do
            if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild("Humanoid") and player.Character.Humanoid.Health > 0 then
                local character = player.Character
                local targetPart = character:FindFirstChild(Mega.States.AimAssist.TargetPart) or character:FindFirstChild("Head") or character:FindFirstChild("HumanoidRootPart")
                local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")

                if targetPart and humanoidRootPart then
                    local distanceToPlayer = (humanoidRootPart.Position - LocalPlayer.Character.HumanoidRootPart.Position).Magnitude

                    if distanceToPlayer <= Mega.States.AimAssist.Range then
                        local isEnemy = true
                        if player.Team and LocalPlayer.Team then
                            isEnemy = player.Team ~= LocalPlayer.Team
                        end

                        if isEnemy then
                            local screenPoint, onScreen = Workspace.CurrentCamera:WorldToViewportPoint(targetPart.Position)

                            if onScreen then
                                local mousePos = UserInputService:GetMouseLocation()
                                local distance = (Vector2.new(mousePos.X, mousePos.Y) - Vector2.new(screenPoint.X, screenPoint.Y)).Magnitude

                                if distance < closestDistance then
                                    closestDistance = distance
                                    closestPlayer = player
                                end
                            end
                        end
                    end
                end
            end
        end

        return closestPlayer
    end
    function AimAtPlayer(player)
        if not player or not player.Character then return end

        local character = player.Character
        local targetPart = character:FindFirstChild(Mega.States.AimAssist.TargetPart) or character:FindFirstChild("Head") or character:FindFirstChild("HumanoidRootPart")
        if not targetPart then return end

        local camera = Workspace.CurrentCamera

        local targetPosition = targetPart.Position
        local direction = (targetPosition - camera.CFrame.Position).Unit

        local currentCFrame = camera.CFrame
        local targetCFrame = CFrame.lookAt(currentCFrame.Position, targetPosition)
        local smoothCFrame = currentCFrame:Lerp(targetCFrame, Mega.States.AimAssist.Smoothness)

        camera.CFrame = smoothCFrame
    end
    function StartAimLoop()
        if AimLoopConnection then
            AimLoopConnection:Disconnect()
        end

        AimLoopConnection = RunService.RenderStepped:Connect(function()
            if Mega.States.AimAssist.Active and Mega.States.AimAssist.Enabled then
                CurrentAimTarget = GetClosestPlayer()
                if CurrentAimTarget then
                    AimAtPlayer(CurrentAimTarget)
                else
                    CurrentAimTarget = nil
                end
            end
        end)
    end
    function StopAimLoop()
        if AimLoopConnection then
            AimLoopConnection:Disconnect()
            AimLoopConnection = nil
        end
        CurrentAimTarget = nil
    end
    --=== ĞœĞ•Ğ“Ğ-Ğ¡Ğ˜Ğ¡Ğ¢Ğ•ĞœĞ ĞĞ‘Ğ ĞĞ‘ĞĞ¢ĞšĞ˜ ===--
    function ToggleMenu()
        TumbaGUI.Enabled = not TumbaGUI.Enabled

        if TumbaGUI.Enabled then
            print(GetText("print_menu_open", VERSION))
            print(GetText("print_created_by", DEVELOPER))
            print(GetText("print_for", SPECIAL_THANKS))
            print(GetText("print_menu_navigate"))
        else
            print(GetText("print_menu_closed"))
        end
    end
    -- ĞĞ±Ñ€Ğ°Ğ±Ğ¾Ñ‚Ñ‡Ğ¸ĞºĞ¸ ĞºĞ»Ğ°Ğ²Ğ¸Ñˆ
    UserInputService.InputBegan:Connect(function(input, gameProcessed)
        if gameProcessed then return end

        local keyName = input.KeyCode.Name
        if not keyName then return end

        if keyName == Mega.States.Keybinds.Menu then
            ToggleMenu()
        end

        if keyName == Mega.States.Keybinds.AimAssist then
            if Mega.States.AimAssist.Enabled then
                Mega.States.AimAssist.Active = not Mega.States.AimAssist.Active

                if Mega.States.AimAssist.Active then
                    StartAimLoop()
                    print(GetText("print_aim_active"))
                else
                    StopAimLoop()
                    print(GetText("print_aim_inactive"))
                end
            end
        end
    end)
    -- Ğ—Ğ°ĞºÑ€Ñ‹Ñ‚Ğ¸Ğµ Ğ¼ĞµĞ½Ñ
    CloseButton.MouseButton1Click:Connect(function()
        ToggleMenu()
    end)
    -- ĞĞ²Ñ‚Ğ¾-Ğ¾Ğ±Ğ½Ğ¾Ğ²Ğ»ĞµĞ½Ğ¸Ğµ Ñ€Ğ°Ğ·Ğ¼ĞµÑ€Ğ¾Ğ² ĞºĞ¾Ğ½Ñ‚ĞµĞ¹Ğ½ĞµÑ€Ğ¾Ğ²
    for _, frame in pairs(TabFrames) do
        local layout = frame:FindFirstChild("UIListLayout")
        if layout then
            layout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
                frame.CanvasSize = UDim2.new(0, 0, 0, layout.AbsoluteContentSize.Y + 20)
            end)
        end
    end

    --=== ĞœĞ•Ğ“Ğ-Ğ˜ĞĞ˜Ğ¦Ğ˜ĞĞ›Ğ˜Ğ—ĞĞ¦Ğ˜Ğ¯ ===--
    CurrentTab = GetText("tab_home")
    TabFrames[GetText("tab_home")].Visible = true
    for _, child in pairs(TabContainer:GetChildren()) do
        if child:IsA("TextButton") then
            if child.Text == GetText("tab_home") then
                child.BackgroundColor3 = Mega.Settings.Menu.AccentColor
            else
                child.BackgroundColor3 = Mega.Settings.Menu.BackgroundColor
            end
        end
    end
    -- Ğ¡Ğ¾Ğ·Ğ´Ğ°ĞµĞ¼ ESP Ğ´Ğ»Ñ Ğ²ÑĞµÑ… Ğ¸Ğ³Ñ€Ğ¾ĞºĞ¾Ğ²
    for _, player in pairs(Players:GetPlayers()) do
        CreateESP(player)
    end
    -- ĞĞ±Ñ€Ğ°Ğ±Ğ¾Ñ‚Ñ‡Ğ¸ĞºĞ¸ Ğ¸Ğ³Ñ€Ğ¾ĞºĞ¾Ğ²
    Players.PlayerAdded:Connect(function(player)
        CreateESP(player)
    end)
    Players.PlayerRemoving:Connect(function(player)
        RemoveESP(player)
    end)
    -- Ğ¡Ğ¾Ğ·Ğ´Ğ°ĞµĞ¼ FOV ĞºÑ€ÑƒĞ³ Ğ´Ğ»Ñ Aim Assist
    CreateAimFOV()
    -- Ğ—Ğ°Ğ¿ÑƒÑĞºĞ°ĞµĞ¼ Ğ¾ÑĞ½Ğ¾Ğ²Ğ½Ğ¾Ğ¹ Ñ†Ğ¸ĞºĞ» Ğ¾Ğ±Ğ½Ğ¾Ğ²Ğ»ĞµĞ½Ğ¸Ñ
    local StartTime = tick()
    Mega.Objects.Connections.Heartbeat = RunService.Heartbeat:Connect(function()
        Mega.Database.Stats.PlayTime = math.floor((tick() - StartTime) / 60)

        -- Ğ›Ğ¾Ğ³Ğ¸ĞºĞ° Ğ¡Ğ»ĞµĞ¶ĞµĞ½Ğ¸Ñ
        if Mega.States.Player.FollowTarget and Mega.States.Player.FollowTarget.Character and Mega.States.Player.FollowTarget.Character:FindFirstChildOfClass("Humanoid") then
            local targetChar = Mega.States.Player.FollowTarget.Character
            local targetRoot = targetChar:FindFirstChild("HumanoidRootPart")

            if targetRoot then
                Workspace.CurrentCamera.CameraType = Enum.CameraType.Scriptable
                Workspace.CurrentCamera.CFrame = targetRoot.CFrame * CFrame.new(0, 5, 15) 
            end

            if Mega.States.Player.FollowTarget.Character:FindFirstChildOfClass("Humanoid").Health <= 0 then
                StopFollow() 
            end
        elseif Mega.States.Player.FollowTarget then
            StopFollow() 
        end

        -- ĞĞ±Ğ½Ğ¾Ğ²Ğ»ĞµĞ½Ğ¸Ğµ ÑĞ¿Ğ¸ÑĞºĞ° Ğ¸Ğ³Ñ€Ğ¾ĞºĞ¾Ğ²
        local usersTab = TabFrames[GetText("tab_users")]
        if TumbaGUI.Enabled and usersTab and usersTab.Visible then
            UpdatePlayerList()
        end

        if Mega.States.ESP.Enabled then
            UpdateESP()
            UpdateESPColors()
        else
            for player, esp in pairs(Mega.Objects.ESP) do
                esp.box.Visible = false
                esp.name.Visible = false
                esp.distance.Visible = false
                esp.health.Visible = false
                esp.tracer.Visible = false
            end
        end

        if AimFOVCircle then
            AimFOVCircle.Visible = Mega.States.AimAssist.Enabled and Mega.States.AimAssist.ShowFOV
            AimFOVCircle.Radius = Mega.States.AimAssist.FOV
            
            local fovColor = Mega.States.AimAssist.FOVColor
            if typeof(fovColor) == "Color3" then
                AimFOVCircle.Color = fovColor
            elseif type(fovColor) == "table" and fovColor.R then
                AimFOVCircle.Color = Color3.new(fovColor.R, fovColor.G, fovColor.B)
            else
                AimFOVCircle.Color = Color3.fromRGB(0, 180, 255)
            end
            
            AimFOVCircle.Position = UserInputService:GetMouseLocation()
        end

        if StatsLabel then
            StatsLabel.Text = GetText("stats_label",
                Mega.Database.Stats.Kills,
                Mega.Database.Stats.Deaths,
                Mega.Database.Stats.PlayTime,
                Mega.Database.Stats.ConfigSaves
            )
        end
        
        -- ĞĞ±Ğ½Ğ¾Ğ²Ğ»ĞµĞ½Ğ¸Ğµ Ğ¸Ğ½Ğ´Ğ¸ĞºĞ°Ñ‚Ğ¾Ñ€Ğ° Ğ°ĞºÑ‚Ğ¸Ğ²Ğ½Ñ‹Ñ… Ñ„ÑƒĞ½ĞºÑ†Ğ¸Ğ¹
        if tick() % 0.5 < 0.016 then
            UpdateStatusIndicator()
        end

        -- === Ğ£Ğ›Ğ£Ğ§Ğ¨Ğ•ĞĞĞĞ¯ Ğ›ĞĞ“Ğ˜ĞšĞ v5: ĞĞĞ¢Ğ˜-Ğ£Ğ ĞĞ ĞĞ¢ ĞŸĞĞ”Ğ•ĞĞ˜Ğ¯ (Ğ­ĞšĞ¡Ğ¢Ğ Ğ•ĞĞĞĞ• Ğ¢ĞĞ ĞœĞĞ–Ğ•ĞĞ˜Ğ•) ===
        if Mega.States.Player.NoFall then
            local character = LocalPlayer.Character
            if character then
                local humanoid = character:FindFirstChildOfClass("Humanoid")
                local rootPart = character:FindFirstChild("HumanoidRootPart")

                if humanoid and rootPart and rootPart:IsA("BasePart") and not rootPart:FindFirstChild("NoFallVelocity") then
                    local isFalling = humanoid:GetState() == Enum.HumanoidStateType.Freefall
                    local fallSpeed = rootPart.AssemblyLinearVelocity.Y

                    -- ĞŸÑ€Ğ¾Ğ²ĞµÑ€ÑĞµĞ¼, Ğ¿Ğ°Ğ´Ğ°ĞµĞ¼ Ğ»Ğ¸ Ğ¼Ñ‹ Ñ Ğ¾Ğ¿Ğ°ÑĞ½Ğ¾Ğ¹ ÑĞºĞ¾Ñ€Ğ¾ÑÑ‚ÑŒÑ
                    if isFalling and fallSpeed < -40 then
                        local rayOrigin = rootPart.Position
                        local rayDirection = Vector3.new(0, -60, 0) -- Ğ›ÑƒÑ‡ Ğ´Ğ¾ÑÑ‚Ğ°Ñ‚Ğ¾Ñ‡Ğ½Ğ¾Ğ¹ Ğ´Ğ»Ğ¸Ğ½Ñ‹
                        
                        local raycastParams = RaycastParams.new()
                        raycastParams.FilterDescendantsInstances = {character}
                        raycastParams.FilterType = Enum.RaycastFilterType.Blacklist

                        local raycastResult = Workspace:Raycast(rayOrigin, rayDirection, raycastParams)

                        -- Ğ•ÑĞ»Ğ¸ Ğ·ĞµĞ¼Ğ»Ñ Ğ±Ğ»Ğ¸Ğ·ĞºĞ¾ (Ğ² Ğ¿Ñ€ĞµĞ´ĞµĞ»Ğ°Ñ… 7 Ğ±Ğ»Ğ¾ĞºĞ¾Ğ²)
                        if raycastResult and raycastResult.Distance <= (7 * 3.5) then -- 7 Ğ±Ğ»Ğ¾ĞºĞ¾Ğ² = 24.5 ÑÑ‚Ğ°Ğ´Ğ°
                            local bodyVelocity = Instance.new("BodyVelocity")
                            bodyVelocity.Name = "NoFallVelocity"
                            bodyVelocity.MaxForce = Vector3.new(0, 9e9, 0)
                            bodyVelocity.Velocity = Vector3.new(0, -15, 0) -- Ğ£ÑÑ‚Ğ°Ğ½Ğ°Ğ²Ğ»Ğ¸Ğ²Ğ°ĞµĞ¼ Ğ±ĞµĞ·Ğ¾Ğ¿Ğ°ÑĞ½ÑƒÑ ÑĞºĞ¾Ñ€Ğ¾ÑÑ‚ÑŒ Ğ¿Ğ°Ğ´ĞµĞ½Ğ¸Ñ
                            bodyVelocity.Parent = rootPart
                            Debris:AddItem(bodyVelocity, 0.2) -- Ğ£Ğ½Ğ¸Ñ‡Ñ‚Ğ¾Ğ¶Ğ°ĞµĞ¼ "Ğ¿Ğ¾Ğ´ÑƒÑˆĞºÑƒ" Ñ‡ĞµÑ€ĞµĞ· 0.2 ÑĞµĞºÑƒĞ½Ğ´Ñ‹
                        end
                    end
                end
            end
        end
    end)

    -- === ĞĞĞ’ĞĞ¯ Ğ›ĞĞ“Ğ˜ĞšĞ: ĞĞ’Ğ¢Ğ-Ğ¤Ğ•Ğ ĞœĞ (v10, ĞœĞĞšĞ¡Ğ˜ĞœĞĞ›Ğ¬ĞĞ ĞĞŸĞ¢Ğ˜ĞœĞ˜Ğ—Ğ˜Ğ ĞĞ’ĞĞĞĞĞ¯) ===
    -- Ğ˜Ğ½Ñ„Ğ¾Ñ€Ğ¼Ğ°Ñ†Ğ¸Ñ Ğ´Ğ»Ñ Ğ¿Ğ¾Ğ»ÑŒĞ·Ğ¾Ğ²Ğ°Ñ‚ĞµĞ»ĞµĞ¹:
    -- ĞĞ° Ğ´Ğ°Ğ½Ğ½Ñ‹Ğ¹ Ğ¼Ğ¾Ğ¼ĞµĞ½Ñ‚ Ğ¿Ğ¾Ğ´Ğ´ĞµÑ€Ğ¶Ğ¸Ğ²Ğ°ĞµÑ‚ÑÑ ÑĞ±Ğ¾Ñ€ ÑĞ»ĞµĞ´ÑƒÑÑ‰Ğ¸Ñ… Ñ€ĞµÑÑƒÑ€ÑĞ¾Ğ²:
    -- 1. Ğ ÑƒĞ´Ğ° (hidden-metal-prompt)
    -- 2. Ğ”ĞµÑ€ĞµĞ²Ğ¾ (treeOrb)
    -- === ĞĞĞ’ĞĞ¯ Ğ›ĞĞ“Ğ˜ĞšĞ: ĞĞ’Ğ¢Ğ-Ğ¤Ğ•Ğ ĞœĞ (v10, ĞœĞĞšĞ¡Ğ˜ĞœĞĞ›Ğ¬ĞĞ ĞĞŸĞ¢Ğ˜ĞœĞ˜Ğ—Ğ˜Ğ ĞĞ’ĞĞĞĞĞ¯) ===
    local VirtualInputManager = game:GetService("VirtualInputManager")
    local lastTriggerTime = 0
    local oreCache = {} -- ĞšÑÑˆ Ğ´Ğ»Ñ Ğ²ÑĞµĞ¹ Ñ€ÑƒĞ´Ñ‹ Ğ½Ğ° ĞºĞ°Ñ€Ñ‚Ğµ
    local lastCacheUpdateTime = 0

    -- ĞœĞµĞ´Ğ»ĞµĞ½Ğ½Ñ‹Ğ¹ ÑĞºĞ°Ğ½ĞµÑ€, ĞºĞ¾Ñ‚Ğ¾Ñ€Ñ‹Ğ¹ Ğ¾Ğ±Ğ½Ğ¾Ğ²Ğ»ÑĞµÑ‚ ĞºÑÑˆ Ñ€ÑƒĞ´Ñ‹ Ñ€Ğ°Ğ· Ğ² 5 ÑĞµĞºÑƒĞ½Ğ´
    Mega.Objects.Connections.AutoFarmCacheUpdater = RunService.Heartbeat:Connect(function()
        if not Mega.States.Misc.AutoFarm then return end
        if tick() - lastCacheUpdateTime > 5 then
            local newCache = {}
            -- Ğ¡Ğ¿Ğ¸ÑĞ¾Ğº Ğ¿Ğ¾Ğ´Ğ´ĞµÑ€Ğ¶Ğ¸Ğ²Ğ°ĞµĞ¼Ñ‹Ñ… Ñ€ĞµÑÑƒÑ€ÑĞ¾Ğ²:
            -- 1. Ğ ÑƒĞ´Ğ° (hidden-metal-prompt)
            -- 2. Ğ”ĞµÑ€ĞµĞ²Ğ¾ (treeOrb)
            for _, prompt in ipairs(Workspace:GetDescendants()) do
                if prompt.Name == "hidden-metal-prompt" and prompt:IsA("ProximityPrompt") then
                    table.insert(newCache, prompt)
                end
            end
            oreCache = newCache
            lastCacheUpdateTime = tick()
        end
    end)

    Mega.Objects.Connections.AutoFarm = RunService.Heartbeat:Connect(function()
        if not Mega.States.Misc.AutoFarm then return end
        if tick() - lastTriggerTime < 0.25 or #oreCache == 0 then return end

        local localRoot = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
        if not localRoot then return end

        for i = #oreCache, 1, -1 do
            local prompt = oreCache[i]
            pcall(function()
                if prompt and prompt.Parent and prompt.Enabled then
                    local orePart = prompt.Parent.PrimaryPart
                    if orePart and (localRoot.Position - orePart.Position).Magnitude <= prompt.MaxActivationDistance then
                        prompt.HoldDuration = 0
                        VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.F, false, game)
                        VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.F, false, game)
                        lastTriggerTime = tick()
                        table.remove(oreCache, i) -- Ğ£Ğ´Ğ°Ğ»ÑĞµĞ¼ Ğ¸Ğ· ĞºÑÑˆĞ°, Ñ‡Ñ‚Ğ¾Ğ±Ñ‹ Ğ½Ğµ ĞºĞ»Ğ¸ĞºĞ°Ñ‚ÑŒ ÑĞ½Ğ¾Ğ²Ğ°
                        return -- Ğ’Ñ‹Ñ…Ğ¾Ğ´Ğ¸Ğ¼ Ğ¸Ğ· pcall Ğ¸ Ñ†Ğ¸ĞºĞ»Ğ°
                    end
                end
            end)
        end
    end)

    -- === ĞĞĞ’ĞĞ¯ Ğ›ĞĞ“Ğ˜ĞšĞ: ĞĞ’Ğ¢Ğ-ĞšĞ˜Ğ¢ ===
    Mega.Objects.Connections.AutoKit = LocalPlayer.CharacterAdded:Connect(function(character)
        -- ĞĞµĞ±Ğ¾Ğ»ÑŒÑˆĞ°Ñ Ğ·Ğ°Ğ´ĞµÑ€Ğ¶ĞºĞ°, Ñ‡Ñ‚Ğ¾Ğ±Ñ‹ Ğ¸Ğ³Ñ€Ğ° ÑƒÑĞ¿ĞµĞ»Ğ° Ğ¿Ñ€Ğ¾Ğ³Ñ€ÑƒĞ·Ğ¸Ñ‚ÑŒ Ğ²ÑĞµ Ğ½ĞµĞ¾Ğ±Ñ…Ğ¾Ğ´Ğ¸Ğ¼Ğ¾Ğµ
        task.wait(2) 
        -- Ğ˜Ğ—ĞœĞ•ĞĞ•ĞĞ: Ğ¢ĞµĞ¿ĞµÑ€ÑŒ Ğ¿Ñ€Ğ¾Ğ²ĞµÑ€ÑĞµĞ¼ ÑĞ¾ÑÑ‚Ğ¾ÑĞ½Ğ¸Ğµ AutoFarm, ĞºĞ¾Ñ‚Ğ¾Ñ€Ğ¾Ğµ Ğ¼Ñ‹ Ğ¿ĞµÑ€ĞµĞ¸Ğ¼ĞµĞ½Ğ¾Ğ²Ğ°Ğ»Ğ¸ Ğ² Auto-Kit
        if Mega.States.Misc.AutoFarm and Mega.States.Misc.AutoKit.KitName ~= "" then
            local kitName = Mega.States.Misc.AutoKit.KitName
            -- [[ Ğ’ĞĞ–ĞĞ! ]]
            -- Ğ’Ğ°Ğ¼ Ğ½ÑƒĞ¶Ğ½Ğ¾ Ğ½Ğ°Ğ¹Ñ‚Ğ¸ Ğ¿Ñ€Ğ°Ğ²Ğ¸Ğ»ÑŒĞ½Ñ‹Ğ¹ RemoteEvent Ğ´Ğ»Ñ Ğ²Ñ‹Ğ±Ğ¾Ñ€Ğ° ĞºĞ¸Ñ‚Ğ°.
            -- ĞĞ±Ñ‹Ñ‡Ğ½Ğ¾ Ğ¾Ğ½ Ğ½Ğ°Ñ…Ğ¾Ğ´Ğ¸Ñ‚ÑÑ Ğ² ReplicatedStorage. ĞŸĞ¾Ğ¸Ñ‰Ğ¸Ñ‚Ğµ Ñ‡Ñ‚Ğ¾-Ñ‚Ğ¾ Ğ²Ñ€Ğ¾Ğ´Ğµ "KitRemote", "GameRemote" Ğ¸Ğ»Ğ¸ "ShopRemote".
            -- Ğ—Ğ°Ñ‚ĞµĞ¼ Ğ¸Ğ·Ğ¼ĞµĞ½Ğ¸Ñ‚Ğµ Ğ¿ÑƒÑ‚ÑŒ "ReplicatedStorage.Game.Events.KitSelect" Ğ½Ğ° Ğ¿Ñ€Ğ°Ğ²Ğ¸Ğ»ÑŒĞ½Ñ‹Ğ¹.            
            local kitRemote = ReplicatedStorage:FindFirstChild("Game", true) and ReplicatedStorage.Game.Events:FindFirstChild("KitSelect")
            if kitRemote then
                kitRemote:FireServer(kitName)
            end
        end
    end)

    -- ĞĞ½Ñ‚Ğ¸-AFK ÑĞ¸ÑÑ‚ĞµĞ¼Ğ°
    if Mega.Settings.System.AntiAFK then
        local VirtualUser = game:GetService("VirtualUser")
        LocalPlayer.Idled:Connect(function()
            VirtualUser:CaptureController()
            VirtualUser:ClickButton2(Vector2.new())
        end)
    end
    -- ĞĞ²Ñ‚Ğ¾-ÑĞ¾Ñ…Ñ€Ğ°Ğ½ĞµĞ½Ğ¸Ğµ ĞºĞ¾Ğ½Ñ„Ğ¸Ğ³Ğ¾Ğ²
    if Mega.Settings.System.AutoSaveConfig then
        -- (Ğ›Ğ¾Ğ³Ğ¸ĞºĞ° Ğ°Ğ²Ñ‚Ğ¾ÑĞ¾Ñ…Ñ€Ğ°Ğ½ĞµĞ½Ğ¸Ñ Ğ¾ÑÑ‚Ğ°ĞµÑ‚ÑÑ Ğ·Ğ´ĞµÑÑŒ)
    end

    -- Ğ¡Ğ¸ÑÑ‚ĞµĞ¼Ğ° cleanup
    local function Cleanup()
        for _, conn in pairs(Mega.Objects.Connections) do
            pcall(function() conn:Disconnect() end)
        end

        for _, esp in pairs(Mega.Objects.ESP) do
            for _, drawing in pairs(esp) do
                pcall(function() drawing:Remove() end)
            end
        end

        TumbaGUI:Destroy()
        showNotification(GetText("notify_cleanup"))
    end

    -- Ğ”Ğ¾Ğ±Ğ°Ğ²Ğ»ÑĞµĞ¼ ĞºĞ½Ğ¾Ğ¿ĞºÑƒ Ğ´Ğ»Ñ cleanup Ğ² Ğ½Ğ°ÑÑ‚Ñ€Ğ¾Ğ¹ĞºĞ¸
    CreateButton(SettingsFrame, "button_cleanup", Cleanup)
    
    -- Ğ˜Ğ—ĞœĞ•ĞĞ•ĞĞ: Ğ˜ÑĞ¿Ğ¾Ğ»ÑŒĞ·ÑƒĞµĞ¼ GetText Ğ´Ğ»Ñ print
    print(GetText("print_load_success", VERSION))
    print(GetText("print_created_by", DEVELOPER))
    print(GetText("print_for", SPECIAL_THANKS))
    print(GetText("print_build_date", BUILD_DATE))
    print(GetText("print_menu_key", Mega.States.Keybinds.Menu))
    print(GetText("print_esp_ready"))
    print(GetText("print_kit_esp_ready"))
    print(GetText("print_aim_ready", Mega.States.Keybinds.AimAssist))
    print(GetText("print_all_ready"))
    
    -- Ğ¡Ğ¾Ñ…Ñ€Ğ°Ğ½ÑĞµĞ¼ Ğ´ĞµÑ„Ğ¾Ğ»Ñ‚Ğ½Ñ‹Ğ¹ ĞºĞ¾Ğ½Ñ„Ğ¸Ğ³
    local configData = {
        Settings = Mega.Settings,
        States = Mega.States,
        Stats = Mega.Database.Stats
    }
    Mega.Database.Configs["default"] = configData
    return Mega

end -- === ĞšĞĞĞ•Ğ¦ Ğ¤Ğ£ĞĞšĞ¦Ğ˜Ğ˜ InitializeMainGUI ===

-- === ĞĞĞ’ĞĞ¯ Ğ¤Ğ£ĞĞšĞ¦Ğ˜Ğ¯ ĞŸĞ•Ğ Ğ•Ğ—ĞĞ“Ğ Ğ£Ğ—ĞšĞ˜ GUI ===
local function ReloadGUI(shouldBeEnabled)
    -- 1. ĞÑ‡Ğ¸Ñ‰Ğ°ĞµĞ¼ Ğ²ÑĞµ ÑÑ‚Ğ°Ñ€Ñ‹Ğµ Ğ¿Ğ¾Ğ´ĞºĞ»ÑÑ‡ĞµĞ½Ğ¸Ñ Ğ¸ Ğ¾Ğ±ÑŠĞµĞºÑ‚Ñ‹
    for _, conn in pairs(Mega.Objects.Connections) do
        pcall(function() conn:Disconnect() end)
    end
    
    for _, esp in pairs(Mega.Objects.ESP) do
        for _, drawing in pairs(esp) do
            pcall(function() drawing:Remove() end)
        end
    end
    
    if Mega.Objects.GUI then
        Mega.Objects.GUI:Destroy()
    end
    
    -- 2. Ğ¡Ğ±Ñ€Ğ°ÑÑ‹Ğ²Ğ°ĞµĞ¼ Ñ‚Ğ°Ğ±Ğ»Ğ¸Ñ†Ñƒ Ğ¾Ğ±ÑŠĞµĞºÑ‚Ğ¾Ğ²
    Mega.Objects = { ESP = {}, KitESPObjects = {}, Connections = {}, GUI = nil, PlayerListItems = {} }
    
    -- 3. Ğ˜Ğ½Ğ¸Ñ†Ğ¸Ğ°Ğ»Ğ¸Ğ·Ğ¸Ñ€ÑƒĞµĞ¼ GUI Ğ·Ğ°Ğ½Ğ¾Ğ²Ğ¾
    InitializeMainGUI()
end

-- === ĞĞĞ§ĞĞ›Ğ: Ğ›ĞĞ“Ğ˜ĞšĞ Ğ’Ğ«Ğ‘ĞĞ Ğ Ğ¯Ğ—Ğ«ĞšĞ (Ğ¡ Ğ¡ĞĞ¥Ğ ĞĞĞ•ĞĞ˜Ğ•Ğœ) ===

-- Ğ¤ÑƒĞ½ĞºÑ†Ğ¸Ñ ÑĞ¾Ñ…Ñ€Ğ°Ğ½ĞµĞ½Ğ¸Ñ ÑĞ·Ñ‹ĞºĞ°
local function SaveLanguage(lang)
    if writefile then
        writefile("TumbaLanguage.txt", lang)
    end
end

-- Ğ¤ÑƒĞ½ĞºÑ†Ğ¸Ñ Ğ·Ğ°Ğ³Ñ€ÑƒĞ·ĞºĞ¸ ÑĞ·Ñ‹ĞºĞ°
local function LoadLanguage()
    if readfile and isfile and isfile("TumbaLanguage.txt") then
        return readfile("TumbaLanguage.txt")
    end
    return nil
end
-- Ğ¤ÑƒĞ½ĞºÑ†Ğ¸Ñ Ğ²Ğ¾ÑÑÑ‚Ğ°Ğ½Ğ¾Ğ²Ğ»ĞµĞ½Ğ¸Ñ Color3 Ğ¸Ğ· Ñ‚Ğ°Ğ±Ğ»Ğ¸Ñ† JSON
local function RestoreColorsGlobal(data)
    if not data then return data end
    
    if type(data) == "table" then
        if data.R and data.G and data.B and type(data.R) == "number" then
            return Color3.new(data.R, data.G, data.B)
        end
        
        for key, value in pairs(data) do
            data[key] = RestoreColorsGlobal(value)
        end
    end
    
    return data
end

-- Ğ¤ÑƒĞ½ĞºÑ†Ğ¸Ñ Ğ°Ğ²Ñ‚Ğ¾Ğ·Ğ°Ğ³Ñ€ÑƒĞ·ĞºĞ¸ ĞºĞ¾Ğ½Ñ„Ğ¸Ğ³Ğ°
local function AutoLoadConfig()
    local fileName = "TumbaConfig_default.json"
    
    if readfile and isfile and isfile(fileName) and HttpService then
        local success, json = pcall(function()
            return readfile(fileName)
        end)
        if success and json and json ~= "" then
            local decoded, result = pcall(function()
                return HttpService:JSONDecode(json)
            end)
            if decoded and result then
                local configData = result
                configData = RestoreColorsGlobal(configData)
                
                if configData.Settings and configData.Settings.Menu then
                    for key, value in pairs(configData.Settings.Menu) do
                        if typeof(value) == "Color3" or (type(value) == "table" and value.R) then
                            Mega.Settings.Menu[key] = typeof(value) == "Color3" and value or Color3.new(value.R, value.G, value.B)
                        else
                            Mega.Settings.Menu[key] = value
                        end
                    end
                    if configData.Settings.System then
                        Mega.Settings.System = configData.Settings.System
                    end
                end
                
                if configData.States then
                    for category, settings in pairs(configData.States) do
                        if Mega.States[category] then
                            for key, value in pairs(settings) do
                                Mega.States[category][key] = value
                            end
                        end
                    end
                end
                
                if configData.Stats then
                    Mega.Database.Stats = configData.Stats
                end
                
                print("ğŸ’¾ Auto-loaded config: default")
                return true
            end
        end
    end
    return false
end

-- ĞŸÑ€Ğ¾Ğ²ĞµÑ€ÑĞµĞ¼, ĞµÑÑ‚ÑŒ Ğ»Ğ¸ ÑĞ¾Ñ…Ñ€Ğ°Ğ½ĞµĞ½Ğ½Ñ‹Ğ¹ ÑĞ·Ñ‹Ğº
local savedLanguage = LoadLanguage()

if savedLanguage then
    -- Ğ•ÑĞ»Ğ¸ ÑĞ·Ñ‹Ğº ÑĞ¾Ñ…Ñ€Ğ°Ğ½ĞµĞ½ - ÑÑ€Ğ°Ğ·Ñƒ Ğ·Ğ°Ğ¿ÑƒÑĞºĞ°ĞµĞ¼ GUI
    Mega.Localization.CurrentLanguage = savedLanguage
    print("Language loaded: " .. savedLanguage)
    
    -- ĞĞ²Ñ‚Ğ¾Ğ·Ğ°Ğ³Ñ€ÑƒĞ·ĞºĞ° ĞºĞ¾Ğ½Ñ„Ğ¸Ğ³Ğ° (ĞµÑĞ»Ğ¸ ĞµÑÑ‚ÑŒ)
    AutoLoadConfig()
    
    InitializeMainGUI()
else
    -- Ğ•ÑĞ»Ğ¸ ÑĞ·Ñ‹ĞºĞ° Ğ½ĞµÑ‚ - Ğ¿Ğ¾ĞºĞ°Ğ·Ñ‹Ğ²Ğ°ĞµĞ¼ Ğ¾ĞºĞ½Ğ¾ Ğ²Ñ‹Ğ±Ğ¾Ñ€Ğ°
    -- 1. Ğ¡Ğ¾Ğ·Ğ´Ğ°ĞµĞ¼ Ğ¾Ñ‚Ğ´ĞµĞ»ÑŒĞ½Ñ‹Ğ¹, Ğ¿Ñ€Ğ¾ÑÑ‚Ğ¾Ğ¹ GUI Ğ´Ğ»Ñ Ğ²Ñ‹Ğ±Ğ¾Ñ€Ğ°
    local LanguagePrompt = Instance.new("ScreenGui")
    LanguagePrompt.Name = "LanguagePrompt"
    LanguagePrompt.Parent = CoreGui
    LanguagePrompt.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

local Background = Instance.new("Frame")
Background.Size = UDim2.new(0, 300, 0, 420) -- Ğ£Ğ²ĞµĞ»Ğ¸Ñ‡ĞµĞ½Ğ° Ğ²Ñ‹ÑĞ¾Ñ‚Ğ°
Background.Position = UDim2.new(0.5, -150, 0.5, -210) -- Ğ¡ĞºĞ¾Ñ€Ñ€ĞµĞºÑ‚Ğ¸Ñ€Ğ¾Ğ²Ğ°Ğ½Ğ° Ğ¿Ğ¾Ğ·Ğ¸Ñ†Ğ¸Ñ
Background.BackgroundColor3 = Color3.fromRGB(20, 20, 30)
Background.BorderSizePixel = 0
local Corner = Instance.new("UICorner")
Corner.CornerRadius = UDim.new(0, 10)
Corner.Parent = Background
Background.Parent = LanguagePrompt

local Title = Instance.new("TextLabel")
Title.Size = UDim2.new(1, 0, 0, 50)
Title.Position = UDim2.new(0, 0, 0, 10) -- Ğ¡Ğ´Ğ²Ğ¸Ğ½ĞµĞ¼ Ğ²Ğ½Ğ¸Ğ·
Title.BackgroundTransparency = 1
Title.TextColor3 = Color3.new(1, 1, 1)
Title.Font = Enum.Font.GothamBold
Title.TextSize = 18
Title.Text = "TUMBA v5.0 - Select Language"
Title.TextXAlignment = Enum.TextXAlignment.Center -- Ğ¦ĞµĞ½Ñ‚Ñ€Ğ¸Ñ€ÑƒĞµĞ¼
Title.Parent = Background

-- === ĞĞĞ’Ğ«Ğ™ ĞšĞĞĞ¢Ğ•Ğ™ĞĞ•Ğ  Ğ”Ğ›Ğ¯ ĞšĞĞĞŸĞĞš ===
local ButtonContainer = Instance.new("Frame")
ButtonContainer.Name = "ButtonContainer"
ButtonContainer.Size = UDim2.new(1, 0, 0, 350) -- Ğ Ğ°Ğ·Ğ¼ĞµÑ€ Ğ´Ğ»Ñ ĞºĞ½Ğ¾Ğ¿Ğ¾Ğº
ButtonContainer.Position = UDim2.new(0, 0, 0, 70) -- ĞŸĞ¾Ğ·Ğ¸Ñ†Ğ¸Ñ Ğ¿Ğ¾Ğ´ Ğ·Ğ°Ğ³Ğ¾Ğ»Ğ¾Ğ²ĞºĞ¾Ğ¼
ButtonContainer.BackgroundTransparency = 1
ButtonContainer.Parent = Background

-- === ĞŸĞ•Ğ Ğ•ĞĞĞ¡Ğ˜Ğœ LAYOUT Ğ’ ĞšĞĞĞ¢Ğ•Ğ™ĞĞ•Ğ  ===
local ButtonLayout = Instance.new("UIListLayout")
ButtonLayout.FillDirection = Enum.FillDirection.Vertical -- Ğ˜Ğ—ĞœĞ•ĞĞ•ĞĞ
ButtonLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
ButtonLayout.VerticalAlignment = Enum.VerticalAlignment.Top -- Ğ˜Ğ—ĞœĞ•ĞĞ•ĞĞ
ButtonLayout.Padding = UDim.new(0, 10) -- Ğ˜Ğ—ĞœĞ•ĞĞ•ĞĞ
ButtonLayout.Parent = ButtonContainer -- <-- Ğ˜Ğ—ĞœĞ•ĞĞ•ĞĞ

local ButtonCornerTemplate = Instance.new("UICorner")
ButtonCornerTemplate.CornerRadius = UDim.new(0, 6)

local EnglishButton = Instance.new("TextButton")
EnglishButton.Name = "English"
EnglishButton.Size = UDim2.new(0, 250, 0, 40) -- Ğ˜Ğ—ĞœĞ•ĞĞ•ĞĞ
EnglishButton.BackgroundColor3 = Color3.fromRGB(0, 100, 200)
EnglishButton.TextColor3 = Color3.new(1, 1, 1)
EnglishButton.Font = Enum.Font.GothamSemibold
EnglishButton.Text = "English"
EnglishButton.Parent = ButtonContainer -- <-- Ğ˜Ğ—ĞœĞ•ĞĞ•ĞĞ
ButtonCornerTemplate:Clone().Parent = EnglishButton

local RussianButton = EnglishButton:Clone()
RussianButton.Name = "Russian"
RussianButton.Text = "Ğ ÑƒÑÑĞºĞ¸Ğ¹"
RussianButton.Parent = ButtonContainer -- <-- Ğ˜Ğ—ĞœĞ•ĞĞ•ĞĞ

local SpanishButton = EnglishButton:Clone()
SpanishButton.Name = "Spanish"
SpanishButton.Text = "EspaÃ±ol"
SpanishButton.Parent = ButtonContainer

local PortugueseButton = EnglishButton:Clone()
PortugueseButton.Name = "Portuguese"
PortugueseButton.Text = "PortuguÃªs"
PortugueseButton.Parent = ButtonContainer

local KoreanButton = EnglishButton:Clone()
KoreanButton.Name = "Korean"
KoreanButton.Text = "í•œêµ­ì–´"
KoreanButton.Parent = ButtonContainer

local JapaneseButton = EnglishButton:Clone()
JapaneseButton.Name = "Japanese"
JapaneseButton.Text = "æ—¥æœ¬èª"
JapaneseButton.Parent = ButtonContainer

    -- 2. Ğ›Ğ¾Ğ³Ğ¸ĞºĞ° Ğ½Ğ°Ğ¶Ğ°Ñ‚Ğ¸Ñ
    local function OnLanguageSelected(lang)
        Mega.Localization.CurrentLanguage = lang
        print("Language set to: " .. lang)
        
        -- Ğ¡Ğ¾Ñ…Ñ€Ğ°Ğ½ÑĞµĞ¼ ÑĞ·Ñ‹Ğº
        SaveLanguage(lang)

        -- Ğ£Ğ½Ğ¸Ñ‡Ñ‚Ğ¾Ğ¶Ğ°ĞµĞ¼ Ğ¾ĞºĞ½Ğ¾ Ğ²Ñ‹Ğ±Ğ¾Ñ€Ğ°
        LanguagePrompt:Destroy()

        -- Ğ—ĞĞŸĞ£Ğ¡ĞšĞĞ•Ğœ Ğ¡ĞĞ—Ğ”ĞĞĞ˜Ğ• ĞĞ¡ĞĞĞ’ĞĞĞ“Ğ GUI
        InitializeMainGUI()
    end

    EnglishButton.MouseButton1Click:Connect(function()
        OnLanguageSelected("en")
    end)

    RussianButton.MouseButton1Click:Connect(function()
        OnLanguageSelected("ru")
    end)

    SpanishButton.MouseButton1Click:Connect(function()
        OnLanguageSelected("es")
    end)

    PortugueseButton.MouseButton1Click:Connect(function()
        OnLanguageSelected("pt")
    end)

    KoreanButton.MouseButton1Click:Connect(function()
        OnLanguageSelected("ko")
    end)

    JapaneseButton.MouseButton1Click:Connect(function()
        OnLanguageSelected("ja")
    end)
end

-- === ĞšĞĞĞ•Ğ¦: Ğ›ĞĞ“Ğ˜ĞšĞ Ğ’Ğ«Ğ‘ĞĞ Ğ Ğ¯Ğ—Ğ«ĞšĞ ===

-- Ğ¡ĞºÑ€Ğ¸Ğ¿Ñ‚ Ñ‚ĞµĞ¿ĞµÑ€ÑŒ Ğ°Ğ²Ñ‚Ğ¾Ğ¼Ğ°Ñ‚Ğ¸Ñ‡ĞµÑĞºĞ¸ Ğ·Ğ°Ğ³Ñ€ÑƒĞ¶Ğ°ĞµÑ‚ ÑĞ¾Ñ…Ñ€Ğ°Ğ½ĞµĞ½Ğ½Ñ‹Ğ¹ ÑĞ·Ñ‹Ğº Ğ¸Ğ»Ğ¸ Ğ¿Ğ¾ĞºĞ°Ğ·Ñ‹Ğ²Ğ°ĞµÑ‚ Ğ¾ĞºĞ½Ğ¾ Ğ²Ñ‹Ğ±Ğ¾Ñ€Ğ°...
