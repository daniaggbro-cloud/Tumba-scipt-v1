    CreateButton(SettingsFrame, "button_change_theme", function()
        local colors = {
            Color3.fromRGB(255, 50, 50), -- ĞšÑ€Ğ°ÑĞ½Ñ‹Ğ¹
            Color3.fromRGB(50, 255, 50), -- Ğ—ĞµĞ»ĞµĞ½Ñ‹Ğ¹
