            -- –î—Ä—É–≥–æ–µ
            ["esp_studs"] = { ru = "%d studs", en = "%d studs", es = "%d studs", pt = "%d studs", ko = "%d Ïä§ÌÑ∞Îìú", ja = "%d „Çπ„Çø„ÉÉ„Éâ" },
