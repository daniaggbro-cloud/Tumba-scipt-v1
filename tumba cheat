        Mega.States.Keybinds.Menu = key
    end)
    local function GetConfigList()
        local configs = {}
        local foundConfigs = {}
        
        foundConfigs["default"] = true
        table.insert(configs, "default")
        
        if listfiles then
            local success, files = pcall(listfiles) -- Ğ˜Ğ©Ğ•Ğœ Ğ’ ĞšĞĞ ĞĞ•Ğ’ĞĞ™ ĞŸĞĞŸĞšĞ•
            if success and files then
                for _, file in pairs(files) do
                    if type(file) == "string" and file:find("TumbaConfig_") and file:find(".json") then
                        local name = file:match("TumbaConfig_(.+)%.json")
                        if name and name ~= "" and not foundConfigs[name] then
                            foundConfigs[name] = true
                            table.insert(configs, name)
                        end
                    end
                end
            end
        end
        
        for configName, _ in pairs(Mega.Database.Configs) do
            if not foundConfigs[configName] then
                foundConfigs[configName] = true
                table.insert(configs, configName)
                print("ğŸ’¾ Found config in memory: " .. configName)
            end
        end
        
        print("ğŸ“‹ Total configs found: " .. #configs)
        return configs
    end
    
    CreateSection(SettingsFrame, "section_settings_config")
    
    local configNameFrame, configNameBox = CreateTextBox(SettingsFrame, "textbox_config_name", GetText("textbox_config_name"))
    local selectedConfigName = "default"
    
    CreateButton(SettingsFrame, "button_config_save", function()
        local configName = configNameBox.Text
        if configName == "" or configName == GetText("textbox_config_name") then
            showNotification(GetText("notify_enter_name"))
            return
        end
        
        local configData = {
            Settings = Mega.Settings,
            States = Mega.States,
            Stats = Mega.Database.Stats,
            Language = Mega.Localization.CurrentLanguage
        }

        Mega.Database.Configs[configName] = configData
        Mega.Database.Stats.ConfigSaves = Mega.Database.Stats.ConfigSaves + 1
        
        if writefile and HttpService then
            local jsonConfig = HttpService:JSONEncode(configData)
            writefile("TumbaConfig_" .. configName .. ".json", jsonConfig)
        end
        
        showNotification(GetText("notify_config_saved", Mega.Database.Stats.ConfigSaves))
        configNameBox.Text = ""
        
        showNotification("âœ… Config saved! Click ğŸ”„ Refresh List to see it")
    end)
    
    local configList = GetConfigList()
    local configDropdown = CreateDropdown(SettingsFrame, "dropdown_config_list", configList, configList[1] or "default", function(selected)
        if selected and selected ~= "" then
            selectedConfigName = selected
            print("ğŸ“‚ Selected config: " .. selected)
        end
    end, false)
    
    CreateButton(SettingsFrame, "button_config_load", function()
        local configData = nil
        local fileName = "TumbaConfig_" .. selectedConfigName .. ".json"
        
        if readfile and isfile and isfile(fileName) and HttpService then
            local success, json = pcall(function()
                return readfile(fileName)
            end)
            if success then
                local decoded = pcall(function()
                    return HttpService:JSONDecode(json)
                end)
                if decoded then
                    configData = HttpService:JSONDecode(json)
                end
            end
        end
        
        if not configData then
            configData = Mega.Database.Configs[selectedConfigName]
        end
        
        if configData then
            configData = RestoreColorsGlobal(configData)
            
            if configData.Settings and configData.Settings.Menu then
                for key, value in pairs(configData.Settings.Menu) do
                    if typeof(value) == "Color3" or (type(value) == "table" and value.R) then
                        Mega.Settings.Menu[key] = typeof(value) == "Color3" and value or Color3.new(value.R, value.G, value.B)
                    else
                        Mega.Settings.Menu[key] = value
                    end
                end
                if configData.Settings.System then
                    Mega.Settings.System = configData.Settings.System
                end
            end
            
            if configData.States then
                for category, settings in pairs(configData.States) do
                    if Mega.States[category] then
                        for key, value in pairs(settings) do
                            Mega.States[category][key] = value
                        end
                    end
                end
            end
            
            if configData.Stats then
                Mega.Database.Stats = configData.Stats
            end
            
            if configData.Language and configData.Language ~= Mega.Localization.CurrentLanguage then
                Mega.Localization.CurrentLanguage = configData.Language
                if writefile then
                    writefile("TumbaLanguage.txt", configData.Language)
                end
            end

            showNotification(GetText("notify_config_loaded"))
            
            task.wait(0.5)
            -- Ğ˜Ğ¡ĞŸĞĞ›Ğ¬Ğ—Ğ£Ğ•Ğœ ĞĞĞ’Ğ£Ğ® Ğ¤Ğ£ĞĞšĞ¦Ğ˜Ğ®
            ReloadGUI()
            
            task.wait(0.5)
            if Mega.Objects.GUI then
                Mega.Objects.GUI.Enabled = true
            end
        else
            showNotification(GetText("notify_config_not_found"))
        end
    end)
    
    CreateButton(SettingsFrame, "button_config_delete", function()
        if selectedConfigName == "default" then
            showNotification("âš ï¸ Cannot delete default config")
            return
        end
        
        local fileName = "TumbaConfig_" .. selectedConfigName .. ".json"
        if delfile and isfile and isfile(fileName) then
            delfile(fileName)
            showNotification(GetText("notify_config_deleted"))
            selectedConfigName = "default"
        end
    end)
    
    CreateButton(SettingsFrame, "button_config_refresh", function()
        showNotification("ğŸ”„ Reloading GUI to refresh config list...")
        task.wait(0.5)
        -- Ğ˜Ğ¡ĞŸĞĞ›Ğ¬Ğ—Ğ£Ğ•Ğœ ĞĞĞ’Ğ£Ğ® Ğ¤Ğ£ĞĞšĞ¦Ğ˜Ğ®
