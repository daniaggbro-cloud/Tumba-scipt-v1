    PlayerItemTemplate.BackgroundColor3 = Color3.fromRGB(30, 35, 50)
    PlayerItemTemplate.BorderSizePixel = 0
    PlayerItemTemplate.Visible = false
    PlayerItemTemplate.Text = "" 
    PlayerItemTemplate.AutoButtonColor = false

