    local languageOptions = {"language_english", "language_russian", "language_spanish", "language_portuguese", "language_korean", "language_japanese"}
    local currentLangKey = "language_english"
    if Mega.Localization.CurrentLanguage == "ru" then currentLangKey = "language_russian"
    elseif Mega.Localization.CurrentLanguage == "es" then currentLangKey = "language_spanish"
    elseif Mega.Localization.CurrentLanguage == "pt" then currentLangKey = "language_portuguese"
    elseif Mega.Localization.CurrentLanguage == "ko" then currentLangKey = "language_korean"
    elseif Mega.Localization.CurrentLanguage == "ja" then currentLangKey = "language_japanese"
    end
    
    CreateDropdown(SettingsFrame, "dropdown_language", languageOptions, currentLangKey, function(selectedKey)
        local newLang = "en"
        if selectedKey == "language_russian" then newLang = "ru"
        elseif selectedKey == "language_spanish" then newLang = "es"
        elseif selectedKey == "language_portuguese" then newLang = "pt"
        elseif selectedKey == "language_korean" then newLang = "ko"
        elseif selectedKey == "language_japanese" then newLang = "ja"
        end
        
        if newLang == Mega.Localization.CurrentLanguage then
            return
        end
        
        Mega.Localization.CurrentLanguage = newLang
        SaveLanguage(newLang) -- Ğ’Ğ¾Ğ·Ğ²Ñ€Ğ°Ñ‰Ğ°ĞµĞ¼ ÑÑ‚Ğ°Ñ€ÑƒÑ Ğ»Ğ¾Ğ³Ğ¸ĞºÑƒ
        
        local langName = GetText(selectedKey)
        showNotification(GetText("notify_language_changed", langName))
        
        task.wait(1)
        
        -- Ğ˜Ğ¡ĞŸĞĞ›Ğ¬Ğ—Ğ£Ğ•Ğœ ĞĞĞ’Ğ£Ğ® Ğ¤Ğ£ĞĞšĞ¦Ğ˜Ğ®
        ReloadGUI()
        
        task.wait(0.5)
        if Mega.Objects.GUI then
            Mega.Objects.GUI.Enabled = true
        end
    end, true)
